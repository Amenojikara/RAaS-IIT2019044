   <div class="row w-100 d-flex justify-content-center mt-3 mt-md-5 mt-sm-3 align-items-start">
       <div class="col-md-8 col-sm-10 justify-content-center mb-4">
         <div class="card p-3">
             <div>
               <h5 class="card-title">Patient_id:{{data._id}}</h5>
               <h6 class="card-subtitle mb-2 text-muted">Name: {{data.name}}</h6>
               <h6 class="card-title text-danger">Email: {{data.email}}</h6>
               <a class="card-link text-center" data-bs-toggle="collapse" data-bs-target="#{{data._id}}">Test Details</a>
             </div>
             <table class="table table-striped collapse fade" id="{{data._id}}">
                 <thead>
                   <tr>
                     <th scope="col">Disease</th>
                     <th scope="col">Disease ID</th>
                     <th scope="col">Doctor</th>
                     <th scope="col">Details</th>
                   </tr>
                 </thead>
                 <tbody>
                 {{#each data.tests}}
                   <tr>
                     <th scope="row">{{this.title}}</th>
                     <td>{{this.id}}</td>
                     <th scope="row">{{this.supervisor_doctor}}</th>
                     <td>
                      <form method="GET" action="./{{this.title}}/{{this.id}}/">
                        <button>Details</button>
                      </form>
                     </td>
                     <!-- <td> <a href="/a"> <button onclick="showDiseaseDetails(`{{this.details}}`)">Details</button></a></td>
                   </tr> -->
                 {{/each}}
                 </tbody>
               </table>
           </div>
         </div>
       </div>
</div>

